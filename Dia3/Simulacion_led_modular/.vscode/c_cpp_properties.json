{
  "version": 4,
  "configurations": [
    {
      "name": "Linux",
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "c11",
      "intelliSenseMode": "linux-gcc-x64",
      "defines": ["_GNU_SOURCE"],
      "includePath": [
        "${workspaceFolder}/include",
        "${workspaceFolder}/**"
      ],
      "browse": {
        "path": [
          "${workspaceFolder}/include",
          "${workspaceFolder}/src"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ]
}
